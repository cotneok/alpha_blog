<h1 align="center">Listing All Categories</h1>
<div align="center">
  <%= will_paginate @categories, list_classes: %w(pagination justify-content-center), previous_label: "Previous", next_label: "Next" %>
  <% @categories.each do |category| %>
    <ul class="listing">
      <div>
        <div class="card col-md-4 col-md-offset-4"
        <li class="category-title">
          <%= link_to "#{category.name}", category_path(category) %>
        </li>
     </div>
    </div>
   </ul>
  <% end %>
  <%= will_paginate @categories, list_classes: %w(pagination justify-content-center), previous_label: "Previous", next_label: "Next" %>
</div>
